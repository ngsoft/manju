{
    "name": "ngsoft/manju",
    "description": "Manju ORM Based on RedBeanPHP, improving the SimpleModel giving it the faculty to store php scalar types and objects into the database",
    "keywords": [
        "orm", "redbean"
    ],
    "homepage": "https://github.com/ngsoft/manju",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymeric Anger",
            "email": "daedelus.git@gmail.com",
            "homepage": "https://github.com/ngsoft"
        }
    ],
    "require-dev": {
        "phan/phan": "^2.2"
    },
    "require": {
        "php": ">=7.1",
        "gabordemooij/redbean": "5.*",
        "kdyby/parse-use-statements": "*"
    },
    "autoload": {
        "psr-4": {
            "Manju\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
