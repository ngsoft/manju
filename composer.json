{
    "name": "ngsoft/manju",
    "description": "Manju ORM Based on RedBeanPHP, improving the SimpleModel",
    "keywords": ["orm", "redbean", "database", "sql", "mysql", "model"],
    "homepage": "https://github.com/ngsoft/manju",
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "require-dev": {

        "psr/log": "*",
        "psr/cache": "*",
        "wixel/gump": "*",
        "psr/container": "^1.0"
    },
    "require": {
        "php": ">=7.1",
        "gabordemooij/redbean": "*",
        "symfony/polyfill-mbstring": "*"
