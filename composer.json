{
    "name": "ngsoft/manju",
    "description": "Manju ORM on top of RedBeanPHP",
    "keywords": [
        "orm", "redbean", "database", "sql", "mysql", "model", "entity"
    ],
    "homepage": "https://github.com/ngsoft/manju",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }
    ],
    "require-dev": {
        "ngsoft/cache": "^1",
        "psr/container": "^1.0",
        "symfony/event-dispatcher": "^5.3"

    },
    "require": {
        "php": ">=7.4",
        "gabordemooij/redbean": ">=5.7",
        "ngsoft/tools": "^2",
        "ngsoft/annotations": "^1",
        "psr/log": "^1.0",
        "fig/cache-util": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\Manju\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
